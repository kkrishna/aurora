[
  {
    "name": "AuroraExecutor",
    "command": [
      "./thermos_executor.pex",
      "--announcer-enable",
      "--anouncer-ensemble",
      "localhost:2181"
    ],
    "resources": [
      {
        "value": "/home/vagrant/aurora/dist/thermos_executor.pex",
        "executable": true,
        "extract": true,
        "cache": true
      }
    ],
    "thermosObserverRoot": "/var/run/thermos",
    "globalContainerMounts": [
    ],
    "overhead": {
      "numCpus": 0.25,
      "diskMB": 0,
      "ramMB": 128,
      "numPorts": 0
    }
  }
]
